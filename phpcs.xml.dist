<?xml version="1.0"?>
<ruleset name="CakePHP elastic-search">
    <config name="installed_paths" value="../../cakephp/cakephp-codesniffer"/>

    <rule ref="CakePHP"/>

    <!-- Necessary for class aliases used for backwards compat -->
    <rule ref="PSR1.Files.SideEffects.FoundWithSymbols">
        <severity>0</severity>
    </rule>
</ruleset>
